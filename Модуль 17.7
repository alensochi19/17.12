per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
money = int(input('Введите сумму депозита (без пробелов и запятых): '))
deposit = list()
for key in per_cent:
    deposit.append(per_cent[key] * money / 100)
print("Возможные варианты % по депозиту =", ' руб.,'.join(map(str, deposit)), 'руб.')
print('Максимальная сумма % по депозиту = ', max(deposit),'руб.')
